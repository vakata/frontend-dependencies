{
    "name": "vakata/frontend-dependencies",
    "description": "Manage front end depedencies from composer",
    "type": "composer-plugin",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vakata/nodejs-installer"
        }
    ],
    "require": {
        "composer-plugin-api": "^2.0.0",
        "mouf/nodejs-installer" : "0.0.8"
    },
    "extra": {
        "class": "vakata\\frontenddependencies\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "vakata\\frontenddependencies\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "mouf/nodejs-installer": true
        }
    }
}
